tListener("selectstart",function(e){ e.preventDefault(); });            }          </script>        </div>                        <div id="js_tags_preview_toast" class="article-tag__error-tips" style="display: none;">预览时标签不可点</div>                        <div id="content_bottom_area" class="rich_media_tool_area"></div>                              </div>    </div>]]></description>
